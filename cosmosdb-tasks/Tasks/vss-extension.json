{
    "manifestVersion": 1,
    "id": "digiflo-cosmosdb-tasks",
    "name": "DF Tasks for Cosmos DB",
    "publisher": "DigifloSolutions",
    "version" : "1.0.26",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "A build and release extension containing Cosmos DB related tasks.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "cosmos db",
        "document db",
        "json"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "DFDataMigrationTool"
        },
        {
            "path": "img",
            "addressable": true
        }
    ],
    "branding": {
        "color": "rgb(215, 40, 47)",
        "theme": "light"
    },
    "links": {
        "home": {
            "uri": "https://github.com/wynandjordaan/vsts-tasks"
        },
        "issues": {
            "uri": "https://github.com/wynandjordaan/vsts-tasks/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/wynandjordaan/vsts-tasks"
    },
    "contributions": [
        {
            "id": "DFDataMigrationTool",
            "description": "DF Cosmos DB Data Migration Tool",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DFDataMigrationTool"
            }
        }
    ]
}